{   
  "navigationFallback": {     
    "rewrite": "/index.html" 
  },
  "routes": [
    {
      "route": "/index.html",
      "rewrite": "/"
    },
    {
      "route": "/player",
      "rewrite": "/"
    },
    {
      "route": "/history"
    },
    {
      "route": "/help"
    },
    {
      "route": "/profile",
      "allowedRoles": [ "authenticated" ]
    },
    {
      "route": "/settings",
      "allowedRoles": [ "authenticated" ]
    },
    {
      "route": "/submit",
      "allowedRoles": [ "authenticated" ]
    },
    {
      "route": "/api/*",
      "rewrite": "/",
      "allowedRoles": [ "authenticated" ]
    }
  ],
  "responseOverrides": {
    "401": {
      "redirect": "/authentication/login",
      "statusCode": 302
    }
  }

}