{
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "routes": [
    {
      "route": "/index.html",
      "rewrite": "/"
    },
    {
      "route": "/player",
      "rewrite": "/"
    },
    {
      "route": "/history"
    },
    {
      "route": "/help"
    },
    {
      "route": "/profile",
      "allowedRoles": [ "authenticated" ]
    },
    {
      "route": "/settings",
      "allowedRoles": [ "authenticated" ]
    },
    {
      "route": "/submit",
      "allowedRoles": [ "authenticated" ],
      "headers": {
        "cache-control": "must-revalidate, max-age=2419200"
      }
    },
    {
      "route": "/api/*",
      "methods": [ "PUT", "POST", "PATCH", "DELETE" ],
      "allowedRoles": [ "authenticated" ]
    }
  ],
  "responseOverrides": {
    "401": {
      "redirect": "/authentication/login",
      "statusCode": 302
    }
  },
  "mimeTypes": {
    ".json": "text/json"
  }
}